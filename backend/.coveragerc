[run]
omit =
    */wsgi.py
    */celery.py
    */migrations/*
    */test*.py
    */__pycache__/*
    */venv/*
    */env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG:
    pass
    except ImportError:
    # type: ignore
    # noqa
    raise PermissionDenied
    return Response
    else:
        return

